<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <servers>
      <server>
        <id>ossrh</id>
        <username>${env.SONATYPE_USERNAME}</username>
        <password>${env.SONATYPE_PASSWORD}</password>
      </server>
  </servers>

  <profiles>
    <profile>
      <id>ossrh</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <gpg.executable>gpg</gpg.executable>
      </properties>
    </profile>
  </profiles>
</settings>
